          <JLabel Columns="14" Background="242,236,224">$Form.VerifiedBy$</JLabel>
          <JTextField ModelPath="Model/Meldebescheinigung/BescheinigteBeteiligten/ed:EinwohnerAngaben/ed:EinwohnerGeburtsdaten/ed:Geburtsdatum/aapd:VerifiedBy" 
                      Columns="14"></JTextField>
        </JPanel>

        <JPanel LayoutClass="FlowLayout" LayoutParameter="FlowLayout.LEFT" Background="242,236,224"
                AlignmentX="0">
          <JLabel Columns="14" Background="242,236,224">$Form.CityName$</JLabel>
          <JTextField ModelPath="Model/Meldebescheinigung/BescheinigteBeteiligten/ed:EinwohnerAngaben/ed:EinwohnerGeburtsdaten/cbc:CityName" 
                      Columns="14"></JTextField>
        </JPanel>

        <JPanel LayoutClass="FlowLayout" LayoutParameter="FlowLayout.LEFT" Background="242,236,224"
                AlignmentX="0">
          <JLabel Columns="14" Background="242,236,224">$Form.BirthActNumber$</JLabel>
          <JTextField ModelPath="Model/Meldebescheinigung/BescheinigteBeteiligten/ed:EinwohnerAngaben/ed:EinwohnerGeburtsdaten/ed:GeburtsurkundeNummer" 
