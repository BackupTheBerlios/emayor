          <JLabel Columns="14" Background="242,236,224">$Form.ValidUntil$</JLabel>
          <JTextField ModelPath="Model/Meldebescheinigung/ed:Gültigkeitszeitraum" 
                      Columns="14"></JTextField>
        </JPanel>

        <Box BoxType="createVerticalStrut" BoxSize="20"/>




        <JPanel LayoutClass="FlowLayout" LayoutParameter="FlowLayout.LEFT" Background="242,236,224"
                AlignmentX="0">
          <JLabel Columns="14" Background="242,236,224">$Form.Postbox$</JLabel>
          <JTextField ModelPath="Model/Meldebescheinigung/Adresse/ed:Postfach" 
